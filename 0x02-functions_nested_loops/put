#include  "main.h"

/**
 * main - A prrogram that prints _putchar
 * and a nwe line
 * Return: Always 0 (Success)
 */
int main(void)
{
        for (int i = 95; i < = 112;  )
        {
                _putchar(i);
                i = i + 17;
                }
        for (int i = 117; i > = 116; i--)
        {

                _putchar(i);
                }
        for (int i = 99; i < = 104; )
        {
                _putchar(i);
                i = i + 5;
        }
        for (int i = 97; i < = 114; )
        {
                _putchar(1);
                i = i + 17;
        }
        _putchar('\n');
        return (0);
}

